non-fluents nf_sysadmin_uniring_2_2_1 {
	domain = sysadmin_mdp;
	objects {
		computer : {c1,c2};
	};
	non-fluents {
		REBOOT-PROB = 0.05;
		CONNECTED(c1,c2);
		CONNECTED(c2,c1);
	};
}

instance sysadmin_uniring_2_2_1 {
	domain = sysadmin_mdp;
	non-fluents = nf_sysadmin_uniring_2_2_1;
	init-state {
		running(c1);
		running(c2);
	};

	max-nondef-actions = 1;
	horizon  = 40;
	discount = 1.0;
}
